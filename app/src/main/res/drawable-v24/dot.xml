<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1dp"
        android:right="1dp"
        android:bottom="1dp"
        android:top="1dp">

        <shape>
            <stroke
                android:width="16dp"
                android:color="#ce454444"
                android:dashGap="4dp"
                android:dashWidth="3dp"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
</layer-list>